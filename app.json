{
    "name": "June-Md",
    "description": "A whatsApp bot based on Baileysjs library with Multi Device support",
    "repository": "https://github.com/Vinpink2/JUNE-MD",
    "logo": "https://files.catbox.moe/7ibt7j.jpg",
    "keywords": ["nodejs", "bot", "whatsapp bot", "whatsapp automation", "multi device"],
    "env": {
    "OWNER_NAME": {
      "description": "enter your owner name here",
      "value": "Supreme",
      "required": false
    },
    "BOT_PREFIX": {
      "description": "Put your bot prefix of desire here",
      "value": ".",
      "required": false
    },
   "AUTO_STATUS": {
      "description": "type true if you want bot to view status if else type false",
      "value": "true",
      "required": false
    },
  "AUTO_READ": {
      "description": "type true if you want bot to read incoming messages if else type false",
      "value": "true",
      "required": false
    },
    "SESSION_ID": {
      "description": "put your session ID here",
      "value": "",
      "required": true
    }
    },
    "buildpacks": [
      {
        "url": "heroku/nodejs"
    },
    {
      "url": "https://github.com/jonathanong/heroku-buildpack-ffmpeg-latest"
    },
    {
      "url": "https://github.com/clhuang/heroku-buildpack-webp-binaries.git"
          }
     ],
     "addons": [
     "heroku-postgresql"
     ]
} 
